  {
    "title": "myAhk on macOS",
    "rules": [
      {
        "description": " change command to control",
        "manipulators": [
          {
            "from": {
              "key_code": "left_control"
            },
            "to": [
              {
                "key_code": "left_gui"
              }
            ],
            "type": "basic"
          },
          {
            "from": {
              "key_code": "left_gui"
            },
            "to": [
              {
                "key_code": "left_control"
              }
            ],
            "type": "basic"
          }
        ]
      },
      {
        "description": "Change Option + i/j/k/l to Arrows",
        "manipulators": [
          {
            "type": "basic",
            "from": {
              "key_code": "j",
              "modifiers": {
                "mandatory": [
                  "left_option"
                ],
                "optional": [
                  "any"
                ]
              }
            },
            "to": [
              {
                "key_code": "left_arrow"
              }
            ]
          },
          {
            "type": "basic",
            "from": {
              "key_code": "k",
              "modifiers": {
                "mandatory": [
                  "left_option"
                ],
                "optional": [
                  "any"
                ]
              }
            },
            "to": [
              {
                "key_code": "down_arrow"
              }
            ]
          },
          {
            "type": "basic",
            "from": {
              "key_code": "i",
              "modifiers": {
                "mandatory": [
                  "left_option"
                ],
                "optional": [
                  "any"
                ]
              }
            },
            "to": [
              {
                "key_code": "up_arrow"
              }
            ]
          },
          {
            "type": "basic",
            "from": {
              "key_code": "l",
              "modifiers": {
                "mandatory": [
                  "left_option"
                ],
                "optional": [
                  "any"
                ]
              }
            },
            "to": [
              {
                "key_code": "right_arrow"
              }
            ]
          }
        ]
      },
      {
        "description": "Change Option + h/n to Home end",
        "manipulators": [
          {
            "type": "basic",
            "from": {
              "key_code": "h",
              "modifiers": {
                "mandatory": [
                  "left_option"
                ],
                "optional": [
                  "any"
                ]
              }
            },
            "to": [
              {
                "key_code": "home"
              }
            ]
          },
          {
            "type": "basic",
            "from": {
              "key_code": "n",
              "modifiers": {
                "mandatory": [
                  "left_option"
                ],
                "optional": [
                  "any"
                ]
              }
            },
            "to": [
              {
                "key_code": "end"
              }
            ]
          }
        ]
      }
    ]
  }